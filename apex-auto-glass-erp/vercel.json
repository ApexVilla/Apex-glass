{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "backend/public/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/public/index.php"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}